        "rules": [
          {
            "description": "In iTerm2/Ghostty: Tap Left Command for tmux prefix (Ctrl+a)",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "left_command",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "left_command"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "a",
                    "modifiers": [
                      "left_control"
                    ]
                  }
                ],
                "conditions": [
                  {
                    "type": "frontmost_application_if",
                    "bundle_identifiers": [
                      "^com\\.googlecode\\.iterm2$",
                      "^com\\.mitchellh\\.ghostty$"
                    ]
                  }
                ]
              }
            ]
          },
          {
            "description": "Tap Right Command for Escape, Hold for Left Control",
            "manipulators": [
              {
                "from": {
                  "key_code": "right_command"
                },
                "to": [
                  {
                    "key_code": "left_control"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
